<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/public/assets/css/material-kit.css">
    <link rel="stylesheet" href="/public/assets/css/style.css">
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    
    <title>web-ios-api</title>
</head>
<body>
    <div class="container-fluid">
        <div class="card card-nav-tabs mrt">
            <div class="card-header card-header-primary">
              <!-- colors: "header-primary", "header-info", "header-success", "header-warning", "header-danger" -->
              <div class="nav-tabs-navigation">
                <div class="nav-tabs-wrapper">
                  <ul class="nav nav-tabs" data-tabs="tabs">
                    <li class="nav-item">
                      <a class="nav-link active" href="#users" data-toggle="tab">
                        <i class="material-icons">face</i> Users
                      </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#remarks" data-toggle="tab">
                          <i class="material-icons">build</i> Remarks
                        </a>
                      </li>
                    <li class="nav-item">
                      <a class="nav-link" href="#answers" data-toggle="tab">
                        <i class="material-icons">chat</i> Answers
                      </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#categories" data-toggle="tab">
                          <i class="material-icons">face</i> Categories
                        </a>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
            <div class="card-body ">
              <div class="tab-content text-center">
                <div class="tab-pane active" id="users">
                    
                  <% routes.users.forEach(route =>{ %>
                    <div class="card card-nav-tabs text-left mrt">
                        <div class="card-header card-header-primary">
                            <span class=
                            <% if (route.type === "get") {  %>
                                "badge badge-pill badge-success"
                            <% } %>
                            <% if (route.type === "post") {  %>
                                "badge badge-pill badge-warning"
                            <% } %>
                            <% if (route.type === "put") {  %>
                                "badge badge-pill badge-info"
                            <% } %>
                            <% if (route.type === "delete") {  %>
                                "badge badge-pill badge-danger"
                            <% } %>
                            '>
                            <%= route.type %></span> <%= route.route %>
                        </div>
                        <div class="card-body">
                          <blockquote class="blockquote mb-0">
                            <p>Description :</p>
                            <p class= "code"><%= route.desc %></p>

                            <% if(route.param) { %>
                            <p>Parameters : </p>
                            <ul class="code">
                                <% route.param.forEach(para => { %>
                                <li><%= para.paramName %> : <%= para.ParamDesc %></li>
                                <% }) %>
                            </ul>
                            <% } %>
                            

                            <% if(route.input) { %>
                            <p>JSON input : </p>
                            <ul class="code">
                                <p>{</p>
                                <% route.input.forEach(inp => { %>
                                <li>"<%= inp.inputName %>" : "<%= inp.inputDesc %>"</li>
                                <% }) %>
                                <p>}</p>
                            </ul>
                            <% } %>

                            

                            <% if(route.output) { %>
                            <p>JSON output : </p>
                            <ul class="code">
                              <p>{</p>
                              <p class="pl10"> "message" :  "<%= route.output.message %>"</p>
                              <% if(route.output.data) { %>
                              <p class="pl10"> "data" :</p>
                              <p class="pl10">{</p>
                              <% route.output.data.forEach(outp => { %>
                              <li class="pl20">"<%= outp.outputName %>" : "<%= outp.outputDesc %>"</li>
                              <% }) %>
                              <p class="pl10">}</p>
                              <% } %>
                              
                              <p>}</p>
                            </ul>
                            <% } %>
                            
                          </blockquote>
                        </div>
                    </div>
                    <% }) %>
